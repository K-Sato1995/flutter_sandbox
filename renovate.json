{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "timezone": "Asia/Tokyo",
  "baseBranches": ["develop"],
  "includePaths": ["pubspec.yaml"],
  "packageRules": [
    {
      "matchUpdateTypes": ["patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "prConcurrentLimit": 5,
  "labels": ["dependencies"],
  "rebaseWhen": "conflicted",
  "reviewers": [
    "K-Sato1995"
  ]
}
